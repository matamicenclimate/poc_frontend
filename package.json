{
  "name": "react-deka-starter",
  "version": "0.9.0",
  "private": true,
  "scripts": {
    "start": "craco start",
    "start:test": "set NODE_ENV=test && craco start",
    "develop": "craco start",
    "start:local": "set NODE_ENV=localhost && craco start",
    "build": "craco build",
    "test": "set NODE_ENV=test && craco test --watchAll=false --silent=false",
    "eject": "react-scripts eject",
    "prepare": "husky install",
    "release": "standard-version",
    "lint": "eslint .",
    "lint:fix": "yarn run lint -- --fix",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public",
    "test-storybook": "test-storybook"
  },
  "dependencies": {
    "@craco/craco": "^6.4.3",
    "@headlessui/react": "^1.5.0",
    "@magic-ext/algorand": "^3.0.1",
    "@popperjs/core": "^2.11.4",
    "@sentry/react": "^6.19.6",
    "@sentry/tracing": "^6.19.6",
    "algosdk": "^1.13.1",
    "axios": "^0.25.0",
    "buffer": "^6.0.3",
    "chart.js": "^3.7.1",
    "clsx": "^1.1.1",
    "crypto-browserify": "^3.12.0",
    "date-fns": "^2.28.0",
    "i18next": "^21.6.11",
    "leaflet": "^1.7.1",
    "magic-sdk": "8.0.1",
    "react": "^18.0.0",
    "react-alert": "^7.0.3",
    "react-chartjs-2": "^4.1.0",
    "react-country-region-selector": "^3.4.0",
    "react-day-picker": "^8.0.0",
    "react-dom": "^18.0.0",
    "react-dropzone": "^12.0.4",
    "react-error-boundary": "^3.1.4",
    "react-helmet-async": "^1.2.3",
    "react-hook-form": "^7.26.1",
    "react-i18next": "^11.15.4",
    "react-leaflet": "4.0.0-alpha.1",
    "react-popper": "^2.2.5",
    "react-popper-tooltip": "^4.3.1",
    "react-query": "^3.34.14",
    "react-query-auth": "^1.1.0",
    "react-router-dom": "6",
    "react-scripts": "5.0.0",
    "react-select": "^5.2.2",
    "standard-version": "^9.3.2",
    "stream-browserify": "^3.0.0",
    "ts-jest": "^27.1.3",
    "typescript": "^4.7.0",
    "web-vitals": "^2.1.4",
    "yup": "^0.32.11"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@commitlint/cli": "^16.2.1",
    "@commitlint/config-conventional": "^16.2.1",
    "@storybook/addon-a11y": "^6.5.9",
    "@storybook/addon-actions": "^6.5.9",
    "@storybook/addon-essentials": "^6.5.9",
    "@storybook/addon-interactions": "^6.5.9",
    "@storybook/addon-links": "^6.5.9",
    "@storybook/builder-webpack5": "^6.5.9",
    "@storybook/manager-webpack5": "^6.5.9",
    "@storybook/node-logger": "^6.5.9",
    "@storybook/preset-create-react-app": "^4.1.2",
    "@storybook/react": "^6.5.9",
    "@storybook/test-runner": "^0.1.1",
    "@storybook/testing-library": "^0.0.13",
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/react": "^13.0.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.4.0",
    "@types/leaflet": "^1.7.9",
    "@types/node": "^16.11.22",
    "@types/react": "^18.0.1",
    "@types/react-alert": "^5.2.1",
    "@types/react-dates": "^21.8.3",
    "@types/react-dom": "^18.0.0",
    "@types/react-router": "^5.1.18",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "^5.11.0",
    "@typescript-eslint/parser": "^5.11.0",
    "autoprefixer": "^10.4.2",
    "babel-plugin-named-exports-order": "^0.0.2",
    "commitlint": "^16.2.1",
    "eslint": "^8.8.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.4.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-storybook": "^0.5.12",
    "eslint-plugin-unused-imports": "^2.0.0",
    "husky": "^7.0.0",
    "msw": "^0.39.2",
    "postcss": "^8.4.6",
    "prettier": "^2.5.1",
    "prettier-plugin-tailwindcss": "^0.1.8",
    "prop-types": "^15.8.1",
    "react-svg-loader": "^3.0.3",
    "tailwindcss": "^3.0.23",
    "webpack": "^5.73.0"
  },
  "jest": {
    "transformIgnorePatterns": [
      "node_modules/(?!react-leaflet)/",
      "/node_modules/(?!react-popper-tooltip)(.*)"
    ],
    "moduleNameMapper": {
      "\\.(css)$": "<rootDir>/src/test/__mocks__/styleMock.ts",
      "^@/(.+)": "<rootDir>/src/$1"
    }
  },
  "msw": {
    "workerDirectory": "public"
  }
}
